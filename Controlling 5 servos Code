#include <Servo.h>;
Servo mservo;
Servo mservo1;
Servo mservo2;
Servo mservo3;
Servo mservo4;

void setup() {
  
  // set up all the LEDs as OUTPUT
  pinMode(2, OUTPUT);
  pinMode(3, OUTPUT);
  pinMode(4, OUTPUT);
  pinMode(5, OUTPUT);
  pinMode(6, OUTPUT);
  
  mservo.attach(2);
  mservo1.attach(3);
  mservo2.attach(4);
  mservo3.attach(5);
  mservo4.attach(6);
 
}

void loop() {
  mservo.write(90);
  mservo1.write(90);
  mservo2.write(90);
  mservo3.write(90);
  mservo4.write(90);
}
